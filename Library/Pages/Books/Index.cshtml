@page
@model Library.Pages.Books.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Books</h1>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Summary</th>
            <th>Dewey Number</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var book in Model.Books) {
            <tr>
                <td>@book.Title</td>
                <td>@book.Summary</td>
                <td>@book.DeweyNumber</td>
            </tr>
        }
    </tbody>
</table>
